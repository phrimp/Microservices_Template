{
  "server": true,
  "node_name": "consul-server-1",
  "bootstrap_expect": 1,
  "data_dir": "/consul/data",
  "client_addr": "0.0.0.0",
  "bind_addr": "0.0.0.0",
  "ui_config": {
    "enabled": true
  },
  "performance": {
    "raft_multiplier": 1
  },
  "acl": {
    "enabled": true,
    "default_policy": "allow",
    "enable_token_persistence": true
  },
  "connect": {
    "enabled": true
  },
  "ports": {
    "grpc": 8502
  },
  "telemetry": {
    "prometheus_retention_time": "30s",
    "disable_hostname": true
  },
  "log_level": "INFO"
}
